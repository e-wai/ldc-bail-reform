{
  "name": "bail-reform-project",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "env-cmd -f .env gatsby build",
    "develop": "env-cmd -f .env gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "@contentful/rich-text-types": "^14.1.1",
    "bootstrap": "^4.5.0",
    "d3": "^5.16.0",
    "gatsby": "^2.23.20",
    "gatsby-background-image": "^1.1.1",
    "gatsby-image": "^2.4.12",
    "gatsby-plugin-react-helmet": "^3.3.9",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sass": "^2.3.10",
    "gatsby-plugin-scroll-reveal": "0.0.7",
    "gatsby-plugin-sharp": "^2.6.17",
    "gatsby-remark-images": "^3.3.17",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-contentful": "^2.3.22",
    "gatsby-source-filesystem": "^2.3.18",
    "gatsby-transformer-remark": "^2.8.23",
    "gatsby-transformer-sharp": "^2.5.10",
    "intersection-observer": "^0.10.0",
    "node-sass": "^4.14.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-jss": "^10.3.0",
    "react-reveal": "^1.2.2",
    "react-scrollama": "^2.2.10",
    "scrollama": "^2.2.1",
    "smooth-scroll": "^16.1.3"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
